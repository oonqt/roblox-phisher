{
  "name": "roblox-phisher",
  "version": "1.0.0",
  "description": "A fullstack Roblox phishing server using NodeJS",
  "scripts": {
    "install:client": "yarn install --cwd client",
    "install:server": "yarn install --cwd server",
    "install:all": "npm-run-all install:client install:server",
    "run:all": "concurrently \"npm start --prefix client\" \"npm run dev --prefix server\""
  },
  "repository": {
    "type": "git",
    "url": "https://git.memester.xyz/rechigo/roblox-phisher.git"
  },
  "keywords": [],
  "author": "rechigo",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "yarn": "^1.22.4"
  }
}
